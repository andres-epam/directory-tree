{
  "name": "directory-tree",
  "version": "1.0.0",
  "description": "Directory tree challenge",
  "main": "lib",
  "scripts": {
    "start": "node lib",
    "test": "jest --coverage",
    "lint": "eslint \"**/*.js\"",
    "format": "prettier --write **/*.js",
    "format-check": "prettier --check **/*.js"
  },
  "keywords": [],
  "author": "Andr√©s Escalante",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2"
  },
  "dependencies": {},
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "collectCoverageFrom": [
      "**/*.js",
      "!./assets/directory.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "testEnvironment": "node"
  }
}
